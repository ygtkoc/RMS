@model Dastone.Models.User

<dl class="row">
    <dt class="col-sm-3">Kullanıcı Adı</dt>
    <dd class="col-sm-9">@Model.Username</dd>
    <dt class="col-sm-3">Adı Soyadı</dt>
    <dd class="col-sm-9">@Model.FirstName @Model.LastName</dd>
    <dt class="col-sm-3">E-posta</dt>
    <dd class="col-sm-9">@Model.Email</dd>
    <dt class="col-sm-3">Rol</dt>
    <dd class="col-sm-9">@Model.Role?.RoleName</dd>
    <dt class="col-sm-3">Oluşturulma Tarihi</dt>
    <dd class="col-sm-9">@Model.CreatedAt.ToString("dd/MM/yyyy HH:mm")</dd>
</dl>