<form id="importFromExcel" asp-action="ImportFromExcel" asp-controller="Vehicles" method="post" enctype="multipart/form-data">
    @Html.AntiForgeryToken()
    <div class="mb-4">
        <label for="excelFile" class="form-label modal-label">Yüklenecek Excel Dosyasını Seçin:</label>
        <input type="file" class="form-control file-input-custom" id="excelFile" name="excelFile" accept=".xlsx" required>
        <span class="text-danger field-validation-valid" data-valmsg-for="excelFile" data-valmsg-replace="true"></span>
    </div>
    <div class="form-message text-center mt-3"></div>
    <div class="">
        <button type="submit" class="btn btn-info">
            <i class="fas fa-upload me-2"></i> Verileri İçe Aktar
        </button>
    </div>
</form>